QPushButton {
    color: white;
    text-align: left;
    border: 2px solid rgba(255, 255, 255, 0);
    border-radius: 4px;
}
QPushButton:hover{
    color: lightblue;
    border: 2px solid rgba(255, 255, 255, 0);
    border-radius: 4px;
}
QPushButton:checked {
    color: blue;
    border: 2px solid rgba(255, 255, 255, 0);
    border-radius: 4px;
}
QFrame#tableItem {
    color: white;
    margin: 10px;
    border: 2px solid rgba(255, 255, 255, 50);
    border-radius: 4px;
}
QFrame#tableItem:hover {
    background-color: rgba(60, 60, 60, 100);
    margin: 10px;
    border: 2px solid rgba(255, 255, 255, 100);
    border-radius: 4px;
}
QPushButton#searchIcon {
    border-image:url(:/img/search.svg);
}
QLabel#titleText {
    color: white;
}
QLabel#Line {
    background-color: qlineargradient(x0:0,y0:0 x1:1,y1:1,
                stop: 0 #000000, stop: 0.4 #1c1c1c,
                stop: 0.5 #1c1c1f, stop: 1.0 #5b5b5b);
    border: 2px solid rgba(255, 255, 255, 10);
    border-radius: 4px;
}
QFrame {
    background-color: rgba(255, 255, 255, 0);
}
QLabel#iconTextLabel {
    color: white;

}
