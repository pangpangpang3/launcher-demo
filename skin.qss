/*InternetApp Button*/
QPushButton#InternetApp{
    border-image:url(:/img/catelogy/internet50.svg);
    border: 1px solid rgba(255, 255, 255, 50);
    border-radius: 0px;
}
QPushButton#InternetApp:checked{
    border-image:url(:/img/catelogy/internet100.svg);
    border: 1px solid rgba(255, 255, 255, 50);
    border-radius: 0px;
}

QPushButton#InternetApp:disabled{
    border-image:url(:/img/catelogy/internet10.svg);
    border: 1px solid rgba(255, 255, 255, 50);
    border-radius: 0px;
}

